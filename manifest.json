{
  "manifest_version": 3,
  "name": "No Addict",
  "version": "1.0",
  "description": "Provide a simple way to delete html on pages you do not want to see anymore",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"],
      "run_at": "document_start"
    }
  ]
}
